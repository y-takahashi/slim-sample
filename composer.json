{
    "name": "y-takahashi/slim-sample",
    "description": "slim sample",
    "type": "library",
    "minimum-stability": "dev",
    "config": {
        "vendor-dir": "vendor"
    },
    "require": {
        "php": ">=5.5.5",
        "slim/slim": "2.3.*",
        "slim/views": "0.1.*",
        "twig/twig": "1.14.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phpunit/dbunit": "1.2.*",
        "phake/phake": "1.*"
    },
    "autoload": {
        "psr-0": {
            "Reboost\\Client": ["src/"],
            "Reboost\\Core": ["src/"]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
